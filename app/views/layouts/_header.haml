%header#header
  .inner
    / Logo
    = link_to root_path, class: 'logo' do
      %span.symbol
        = image_tag 'logo.png'
      %span.title Cat of the Day
    / Nav
    %nav
      %ul
        %li
          %a{:href => "#menu"} Menu
/ Menu
%nav#menu
  %h2 Menu
  %ul
    %li
      = render "shared/menu/#{current_namespace}"
