%section
  %h4 Statistic for all time
  %canvas#account-chart.chart{ 'data-chart' => @chart_data }
%br
%h4 Latest Social Posts
- if @social_posts.present?
  %section.tiles
    - @social_posts.each do |social_post|
      = render 'day_subjects/show/shared/item', social_post: social_post
- else
  %p Not found anything
